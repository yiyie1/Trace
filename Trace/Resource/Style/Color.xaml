	<SolidColorBrush x:Key="Brush_WndBG" Color="#CC000000"/>
	<SolidColorBrush x:Key="Brush_GridSplitter" Color="#FF505050"/>
	<SolidColorBrush x:Key="Brush_Selected" Color="#FFED8216"/>
	<SolidColorBrush x:Key="Brush_WndBG_Blend2Video" Color="#97000000"/>
	<LinearGradientBrush x:Key="Brush_SliderIndicator" EndPoint="0.5,1" StartPoint="0.5,0">
