		<GradientStop Color="#FF0775CA" Offset="1"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="Brush_BGNoTransparent" EndPoint="0.5,1" StartPoint="0.5,0">
		<GradientStop Color="#FF838383" Offset="0"/>
